{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["DOM", "ESNext"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "baseUrl": "./app/javascript",
      "outDir": "./dist"
    },
    "include": ["**/*.tsx", "**/*.ts"]
}
  